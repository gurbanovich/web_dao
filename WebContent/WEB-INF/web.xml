<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"> 
 <display-name>Students personnel</display-name>
  
  <servlet>
    <servlet-name>homeServlet</servlet-name>
    <servlet-class>student.web.HomeServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>studentServlet</servlet-name>
    <servlet-class>student.web.StudentServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>subjectServlet</servlet-name>
    <servlet-class>student.web.SubjectServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>markServlet</servlet-name>
    <servlet-class>student.web.MarkServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>homeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>studentServlet</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>subjectServlet</servlet-name>
    <url-pattern>/subject</url-pattern>
  </servlet-mapping> 
  <servlet-mapping>
    <servlet-name>markServlet</servlet-name>
    <url-pattern>/mark</url-pattern>
  </servlet-mapping> 
  
  
  </web-app>